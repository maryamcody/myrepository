def swap1(a,b):

    a=a^b
    b=a^b
    a=a^b
    print("After swaping a: ",a,"and b: ",b)
a=int(input("Enter a number "))
b=int(input("Enter a number "))
swap1(a,b)

def swap2(a,b):
    a=(a&b) + (a|b)
    b=a+(~b)+1
    a=a+(~b)+1  
    print("After swaping a:",a,"b:",b)
a=int(input("enter A"))
b=int(input("Enter B"))
swap2(a,b)